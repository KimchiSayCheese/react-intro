{
	"name": "intro-to-react",
	"version": "6.0.0",
	"description": "learning react with fem",
	"main": "App.js",
	"repository": "git@github.com:KimchiSayCheese/react-intro.git",
	"author": "Paul Chon",
	"license": "MIT",
	"scripts": {
		"format": "prettier --write \"src/**/*.{js, jsx}\"",
		"lint": "eslint \"src/**/*.{js, jsx}\" --quiet",
		"dev": "parcel src/index.html",
		"build": "parcel build src/index.html && parcel watch src/index.html",
		"no-cache": "parcel --no-cache src/index.html"
	},
	"devDependencies": {
		"@babel/core": "7.12.16",
		"@babel/eslint-parser": "7.13.4",
		"@babel/plugin-proposal-class-properties": "7.13.0",
		"@babel/preset-env": "7.13.5",
		"@babel/preset-react": "7.12.13",
		"@parcel/babel-preset-env": "^2.2.1",
		"eslint": "7.18.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-react": "7.22.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"parcel": "^2.2.1",
		"prettier": "^2.5.1"
	},
	"dependencies": {
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"react-router-dom": "6.2.1"
	},
	"browserslist": [
		"last 2 Chrome versions"
	]
}
